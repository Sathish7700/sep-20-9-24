pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo "hi"'                                                          sh 'echo"hi"'       (wrong)
            }
        }
        stage('Test') {
            steps {
                sh 'echo "hello"'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "hey"'
            }
        }
    }
}